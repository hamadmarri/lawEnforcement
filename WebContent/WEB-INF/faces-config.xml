<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">


	<navigation-rule>
		<from-view-id>/management/entries/person/editPerson.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/management/entries/person/viewPerson.xhtml</to-view-id>
			<redirect include-view-params="true">
				<view-param>
					<name>id</name>
					<value>#{controllerPerson.id}</value>
				</view-param>
			</redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/management/entries/conveyance/editConveyance.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/management/entries/conveyance/viewConveyance.xhtml</to-view-id>
			<redirect include-view-params="true">
				<view-param>
					<name>id</name>
					<value>#{controllerConveyance.id}</value>
				</view-param>
			</redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>test</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/management/view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>fail</from-outcome>
			<to-view-id>/login/forgotpwd.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
